@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

<http://example.org/passthrough>
    a lv2:Plugin , lv2:Effect ;
    doap:name "Passthrough" ;
    lv2:port [
        a lv2:AudioPort , lv2:InputPort ;
        lv2:index 0 ;
        lv2:symbol "in_l" ;
        lv2:name "Input L"
    ] , [
        a lv2:AudioPort , lv2:InputPort ;
        lv2:index 1 ;
        lv2:symbol "in_r" ;
        lv2:name "Input R"
    ] , [
        a lv2:AudioPort , lv2:OutputPort ;
        lv2:index 2 ;
        lv2:symbol "out_l" ;
        lv2:name "Output L"
    ] , [
        a lv2:AudioPort , lv2:OutputPort ;
        lv2:index 3 ;
        lv2:symbol "out_r" ;
        lv2:name "Output R"
    ] ,

    # === Control Parameters ===
    [
        a lv2:ControlPort , lv2:InputPort ;
        lv2:index 4 ;
        lv2:symbol "drive" ;
        lv2:name "Drive" ;
        lv2:default 0.0 ;
        lv2:minimum -1.0 ;
        lv2:maximum 6.0 ;
    ] , [
        a lv2:ControlPort , lv2:InputPort ;
        lv2:index 5 ;
        lv2:symbol "mix" ;
        lv2:name "Mix" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] , [
        a lv2:ControlPort , lv2:InputPort ;
        lv2:index 6 ;
        lv2:symbol "mode" ;
        lv2:name "Mode" ;
        lv2:portProperty lv2:enumeration ;
        lv2:default 0 ;
        lv2:scalePoint [ rdfs:label "Hard" ; rdf:value 0 ] ;
        lv2:scalePoint [ rdfs:label "Soft" ; rdf:value 1 ] ;
        lv2:scalePoint [ rdfs:label "Fold" ; rdf:value 2 ] ;
    ] .

