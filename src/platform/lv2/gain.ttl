@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://example.org/plugins/mygain>
    a lv2:AmplifierPlugin ;
    lv2:project <http://example.org/> ;
    rdfs:label "Tiny Drive" ;
    lv2:port [
        a lv2:AudioPort , lv2:InputPort ;
        lv2:index 0 ;
        lv2:symbol "input" ;
        lv2:name "Input"
    ] , [
        a lv2:AudioPort , lv2:OutputPort ;
        lv2:index 1 ;
        lv2:symbol "output" ;
        lv2:name "Output"
    ] , [
        a lv2:ControlPort , lv2:InputPort ;
        lv2:index 2 ;
        lv2:symbol "gain" ;
        lv2:name "Gain" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 2.0
    ] .
